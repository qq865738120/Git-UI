		"update-notifier": "^5.1.0"
	},
	"devDependencies": {
		"@ava/typescript": "^2.0.0",
		"@sindresorhus/tsconfig": "^2.0.0",
		"@types/react": "^17.0.24",
		"@types/shelljs": "^0.8.9",
		"@typescript-eslint/eslint-plugin": "^4.31.2",
		"@typescript-eslint/parser": "^4.31.2",
		"ava": "^3.15.0",
		"chalk": "^4.1.2",
